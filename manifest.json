{
    "manifest_version": 2,
    "name": "Dim",
    "description": "Dim",
    "version": "0.0.1",
    "author": "Purusah",
    "icons": {
        "48": "icons/border-48.png"
    },
    "background": {
        "scripts": [
            "src/background/index.js"
        ]
    },
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "19": "icons/button-skip-19.png",
            "38": "icons/button-skip-38.png"
        },
        "default_title": "Skip"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "src/content/index.js"
            ]
        }
    ],
    "permissions": [
        "<all_urls>",
        "activeTab",
        "webRequest",
        "webRequestBlocking"
    ]
}